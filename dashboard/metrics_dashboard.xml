<form version="1.1" theme="dark">
  <label>SC4S Metrics</label>
  <description>Metrics documentation: https://axoflow.com/docs/axosyslog-core/chapter-log-statistics/log-statistics-description/</description>
  <search id="baseSearch">
    <query>
    | mstats 
    max("spl.sc4syslog.center.received.processed")
    max("spl.sc4syslog.center.queued.processed")
    max("spl.sc4syslog.dst.batch_size_avg")
    max("spl.sc4syslog.dst.batch_size_max")
    max("spl.sc4syslog.dst.dropped")
    max("spl.sc4syslog.dst.memory_usage")
    max("spl.sc4syslog.dst.written")
    max("spl.sc4syslog.dst.processed")
    max("spl.sc4syslog.dst.queued")
    max("spl.sc4syslog.dst.dropped")
    max("spl.sc4syslog.dst.msg_size_avg")
    max("spl.sc4syslog.src.msg_size_avg")
    max("spl.sc4syslog.global.internal_source.queued")
    max("spl.sc4syslog.global.internal_source.dropped")
    max("spl.sc4syslog.global.internal_source.processed")
    prestats=true
    WHERE "index"="_metrics"
    span=10s
  </query>
    <earliest>$time_range.earliest$</earliest>
    <latest>$time_range.latest$</latest>
    <sampleRatio>1</sampleRatio>
  </search>
  <fieldset submitButton="false">
    <input type="time" token="time_range">
      <label>Time range</label>
      <default>
        <earliest>rt-60m</earliest>
        <latest>rt</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Total processed</title>
      <html>
      <u1> </u1>
    </html>
    </panel>
  </row>
  <row>
    <panel>
      <title>Received</title>
      <html>
        <p>Total messages received from all sources, including metric messages. This count will gradually increase, even if no data is sent to SC4S, until SC4S is restarted.</p>
      </html>
      <chart>
        <title>center.received.processed</title>
        <search base="baseSearch">
          <query>| timechart max("spl.sc4syslog.center.received.processed") AS Total span=10s | fields - _span*</query>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.abbreviation">none</option>
